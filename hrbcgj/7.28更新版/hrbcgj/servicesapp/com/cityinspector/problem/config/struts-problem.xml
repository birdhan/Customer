<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="problem" extends="globalPackage" namespace="/problem">
		<action name="*" class="com.cityinspector.problem.action.problemAction" method="{1}">
			<result name="edit">/WEB-INF/pages/problem/problemForm.jsp</result>
			<result name="detail">/WEB-INF/pages/problem/problemDetail.jsp</result>
			<result name="list">/WEB-INF/pages/problem/problemList.jsp</result>
			<result name="import">/WEB-INF/pages/problem/problemImport.jsp</result>
			<result name="export">/WEB-INF/pages/problem/problemExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/problem/success.do</result>
		</action>
	</package>
</struts>
