<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="viewmesasge" extends="globalPackage" namespace="/viewmesasge">
		<action name="*" class="com.cityinspector.viewmesasge.action.ViewMessageAction" method="{1}">
			<result name="edit">/WEB-INF/pages/viewmesasge/viewMessageForm.jsp</result>
			<result name="detail">/WEB-INF/pages/viewmesasge/viewMessageDetail.jsp</result>
			<result name="list">/WEB-INF/pages/viewmesasge/viewMessageList.jsp</result>
			<result name="import">/WEB-INF/pages/viewmesasge/viewMessageImport.jsp</result>
			<result name="export">/WEB-INF/pages/viewmesasge/viewMessageExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/viewmesasge/success.do</result>
		</action>
	</package>
</struts>
