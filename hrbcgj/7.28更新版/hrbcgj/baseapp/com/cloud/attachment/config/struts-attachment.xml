<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="attachment" extends="globalPackage" namespace="/attachment">
		<action name="*" class="com.cloud.attachment.action.AttachmentAction" method="{1}">
			<result name="edit">/WEB-INF/pages/attachment/attachmentForm.jsp</result>
			<result name="detail">/WEB-INF/pages/attachment/attachmentDetail.jsp</result>
			<result name="list">/WEB-INF/pages/attachment/attachmentList.jsp</result>
			<result name="import">/WEB-INF/pages/attachment/attachmentImport.jsp</result>
			<result name="export">/WEB-INF/pages/attachment/attachmentExport.jsp</result>
			<result name="openAttachmentWin">/WEB-INF/pages/attachment/openAttachmentWin.jsp</result>
			<result name="redirectSuccess" type="redirect">/attachment/success.do</result>
		</action>
	</package>
</struts>
