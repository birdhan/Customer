<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="registeruser" extends="globalPackage" namespace="/registeruser">
		<action name="*" class="com.cityinspector.registeruser.action.RegisterUserAction" method="{1}">
			<result name="edit">/WEB-INF/pages/registeruser/registerUserForm.jsp</result>
			<result name="detail">/WEB-INF/pages/registeruser/registerUserDetail.jsp</result>
			<result name="list">/WEB-INF/pages/registeruser/registerUserList.jsp</result>
			<result name="import">/WEB-INF/pages/registeruser/registerUserImport.jsp</result>
			<result name="export">/WEB-INF/pages/registeruser/registerUserExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/registeruser/success.do</result>
		</action>
	</package>
</struts>
