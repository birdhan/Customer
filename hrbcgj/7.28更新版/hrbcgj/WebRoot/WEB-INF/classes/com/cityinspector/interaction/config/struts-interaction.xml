<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="interaction" extends="globalPackage" namespace="/interaction">
		<action name="*" class="com.cityinspector.interaction.action.InteractionAction" method="{1}">
			<result name="edit">/WEB-INF/pages/interaction/interactionForm.jsp</result>
			<result name="detail">/WEB-INF/pages/interaction/interactionDetail.jsp</result>
			<result name="list">/WEB-INF/pages/interaction/interactionList.jsp</result>
			<result name="import">/WEB-INF/pages/interaction/interactionImport.jsp</result>
			<result name="export">/WEB-INF/pages/interaction/interactionExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/interaction/success.do</result>
		</action>
	</package>
</struts>
