<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="section" extends="globalPackage" namespace="/section">
		<action name="*" class="com.cityinspector.section.action.SectionAction" method="{1}">
			<result name="edit">/WEB-INF/pages/section/sectionForm.jsp</result>
			<result name="detail">/WEB-INF/pages/section/sectionDetail.jsp</result>
			<result name="list">/WEB-INF/pages/section/sectionList.jsp</result>
			<result name="import">/WEB-INF/pages/section/sectionImport.jsp</result>
			<result name="export">/WEB-INF/pages/section/sectionExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/section/success.do</result>
		</action>
	</package>
</struts>
