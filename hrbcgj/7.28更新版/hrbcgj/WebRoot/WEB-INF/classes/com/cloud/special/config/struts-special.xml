<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="special" extends="globalPackage" namespace="/special">
		<action name="*" class="com.cloud.special.action.specialAction" method="{1}">
			<result name="edit">/WEB-INF/pages/special/specialForm.jsp</result>
			<result name="detail">/WEB-INF/pages/special/specialDetail.jsp</result>
			<result name="list">/WEB-INF/pages/special/specialList.jsp</result>
			<result name="import">/WEB-INF/pages/special/specialImport.jsp</result>
			<result name="export">/WEB-INF/pages/special/specialExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/special/success.do</result>
		</action>
	</package>
</struts>
