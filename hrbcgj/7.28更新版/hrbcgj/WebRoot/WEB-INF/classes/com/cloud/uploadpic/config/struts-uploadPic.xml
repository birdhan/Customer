<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="uploadpic" extends="globalPackage" namespace="/uploadpic">
		<action name="*" class="com.cloud.uploadpic.action.UploadPicAction" method="{1}">
			<result name="edit">/WEB-INF/pages/uploadpic/uploadPicForm.jsp</result>
			<result name="detail">/WEB-INF/pages/uploadpic/uploadPicDetail.jsp</result>
			<result name="list">/WEB-INF/pages/uploadpic/uploadPicList.jsp</result>
			<result name="import">/WEB-INF/pages/uploadpic/uploadPicImport.jsp</result>
			<result name="export">/WEB-INF/pages/uploadpic/uploadPicExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/uploadpic/success.do</result>
		</action>
	</package>
</struts>
