<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="schedulemanager" extends="globalPackage" namespace="/schedulemanager">
		<action name="*" class="com.cloud.schedulemanager.action.ScheduleAction" method="{1}">
			<result name="edit">/WEB-INF/pages/schedulemanager/scheduleForm.jsp</result>
			<result name="detail">/WEB-INF/pages/schedulemanager/scheduleDetail.jsp</result>
			<result name="list">/WEB-INF/pages/schedulemanager/scheduleList.jsp</result>
			<result name="import">/WEB-INF/pages/schedulemanager/scheduleImport.jsp</result>
			<result name="export">/WEB-INF/pages/schedulemanager/scheduleExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/schedulemanager/success.do</result>
		</action>
	</package>
</struts>
