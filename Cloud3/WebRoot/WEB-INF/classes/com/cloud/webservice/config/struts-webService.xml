<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="webservice" extends="globalPackage" namespace="/webservice">
		<action name="*" class="com.cloud.webservice.action.WebServiceAction" method="{1}">
			<result name="edit">/WEB-INF/pages/webservice/webServiceForm.jsp</result>
			<result name="detail">/WEB-INF/pages/webservice/webServiceDetail.jsp</result>
			<result name="list">/WEB-INF/pages/webservice/webServiceList.jsp</result>
			<result name="import">/WEB-INF/pages/webservice/webServiceImport.jsp</result>
			<result name="export">/WEB-INF/pages/webservice/webServiceExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/webservice/success.do</result>
		</action>
	</package>
</struts>
