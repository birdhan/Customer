<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="menumanager" extends="globalPackage" namespace="/menumanager">
		<action name="*" class="com.cloud.menumanager.action.MenuAction" method="{1}">
			<result name="edit">/WEB-INF/pages/menumanager/menuForm.jsp</result>
			<result name="detail">/WEB-INF/pages/menumanager/menuDetail.jsp</result>
			<result name="list">/WEB-INF/pages/menumanager/menuList.jsp</result>
			<result name="import">/WEB-INF/pages/menumanager/menuImport.jsp</result>
			<result name="export">/WEB-INF/pages/menumanager/menuExport.jsp</result>
			<result name="view">/WEB-INF/pages/menumanager/menuView.jsp</result>
			<result name="redirectSuccess" type="redirect">/menumanager/success.do</result>
		</action>
	</package>
</struts>
