<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="datadict" extends="globalPackage" namespace="/datadict">
		<action name="*" class="com.cloud.datadict.action.DataDictAction" method="{1}">
			<result name="edit">/WEB-INF/pages/datadict/dataDictForm.jsp</result>
			<result name="detail">/WEB-INF/pages/datadict/dataDictDetail.jsp</result>
			<result name="list">/WEB-INF/pages/datadict/dataDictList.jsp</result>
			<result name="import">/WEB-INF/pages/datadict/dataDictImport.jsp</result>
			<result name="export">/WEB-INF/pages/datadict/dataDictExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/datadict/success.do</result>
		</action>
	</package>
</struts>
