<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="systemuser" extends="globalPackage" namespace="/systemuser">
		<action name="*" class="com.cloud.systemuser.action.SystemUserAction" method="{1}">
			<result name="edit">/WEB-INF/pages/systemuser/systemUserForm.jsp</result>
			<result name="detail">/WEB-INF/pages/systemuser/systemUserDetail.jsp</result>
			<result name="list">/WEB-INF/pages/systemuser/systemUserList.jsp</result>
			<result name="import">/WEB-INF/pages/systemuser/systemUserImport.jsp</result>
			<result name="export">/WEB-INF/pages/systemuser/systemUserExport.jsp</result>
			<result name="userTree">/commonjsp/userTree.jsp</result>
			<result name="redirectSuccess" type="redirect">/systemuser/success.do</result>
		</action>
	</package>
</struts>
