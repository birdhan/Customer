<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="callwebservice" extends="globalPackage" namespace="/callwebservice">
		<action name="*" class="com.cloud.callwebservice.action.CallWebServiceAction" method="{1}">
			<result name="edit">/WEB-INF/pages/callwebservice/callWebServiceForm.jsp</result>
			<result name="detail">/WEB-INF/pages/callwebservice/callWebServiceDetail.jsp</result>
			<result name="list">/WEB-INF/pages/callwebservice/callWebServiceList.jsp</result>
			<result name="import">/WEB-INF/pages/callwebservice/callWebServiceImport.jsp</result>
			<result name="export">/WEB-INF/pages/callwebservice/callWebServiceExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/callwebservice/success.do</result>
		</action>
	</package>
</struts>
