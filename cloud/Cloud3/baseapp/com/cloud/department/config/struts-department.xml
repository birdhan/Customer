<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="department" extends="globalPackage" namespace="/department">
		<action name="*" class="com.cloud.department.action.DepartmentAction" method="{1}">
			<result name="edit">/WEB-INF/pages/department/departmentForm.jsp</result>
			<result name="detail">/WEB-INF/pages/department/departmentDetail.jsp</result>
			<result name="list">/WEB-INF/pages/department/departmentList.jsp</result>
			<result name="import">/WEB-INF/pages/department/departmentImport.jsp</result>
			<result name="export">/WEB-INF/pages/department/departmentExport.jsp</result>
			<result name="tree">/WEB-INF/pages/department/departmentTree.jsp</result>
			<result name="deptTree">/commonjsp/deptTree.jsp</result>
			<result name="redirectSuccess" type="redirect">/department/success.do</result>
		</action>
	</package>
</struts>
