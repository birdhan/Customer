			<result name="edit">/WEB-INF/pages/rolemenupriv/roleMenuPrivForm.jsp</result>
			<result name="detail">/WEB-INF/pages/rolemenupriv/roleMenuPrivDetail.jsp</result>
			<result name="list">/WEB-INF/pages/rolemenupriv/roleMenuPrivList.jsp</result>
			<result name="import">/WEB-INF/pages/rolemenupriv/roleMenuPrivImport.jsp</result>
			<result name="export">/WEB-INF/pages/rolemenupriv/roleMenuPrivExport.jsp</result>
			<result name="redirectSuccess" type="redirect">/rolemenupriv/success.do</result>
		</action>
	</package>
